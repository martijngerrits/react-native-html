{
  "name": "@react-native-html/parser",
  "version": "0.0.24",
  "description": "Parses html into data nodes that are structured for easy rendering in react native",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "react-native": "src/index.tsx",
  "author": "Derk Nolte",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "entities": "^2.0.0",
    "htmlparser2-without-node-native": "^3.9.2",
    "domhandler": "^3.0.0",
    "domutils": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4"
  },
  "scripts": {
    "build": "yarn bob build",
    "prepublishOnly": "yarn --cwd ../../ lint && yarn build && cp ../../README.md ./README.md",
    "test": "jest",
    "prepare": "yarn --cwd ../../ patch-package"
  },
  "files": [
    "src",
    "dist"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "tsc": "../../node_modules/.bin/tsc"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
